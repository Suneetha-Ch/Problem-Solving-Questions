private static void findSumOfDigitsOfString(string s)
        {
            int sum = 0;
            for(int i = 0 ; i < s.Length; ++i)
            {
                if(s[i]>'0' && s[i] < '9')
                {
                    sum += s[i] - '0';
                }
            }
            Console.WriteLine("sum of digits of {0} is:{1} ", s, sum);

        }
